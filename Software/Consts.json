  "pdf_generation_failed": 303,
  "image_generation_failed_bad_input": 304,
  "image_generation_failed": 305,
  "status_codes": {
    "upload_request_success": 201,
    "bad_input": 400,
    "data_removed_code": 410,
    "files_too_large_code": 413,
    "unsupported_media_type_code": 415,
    "internal_server_error_code": 500
  },
  "split_py_stdout_char": ",",
  "id_key_in_jData": "id",
  "media_key_in_jData": "mediaName",
  "download_key_in_jData": "downloadName",
  "midi_key_in_jData": "midiName",
  "pdf_key_in_jData": "pdfName",
  "img_key_in_jData": "imageNames",
  "pdf_ext": ".pdf",
  "img_ext": ".jpeg",
  "midi_ext": ".mid",
  "valid_audio_extensions": [ ".mp3", ".ogg", ".wav", ".flac", ".m4a", ".mpeg", ".aac" ],
  "max_files_transfer": 1,
  "max_size_Bytes": 60000000,
  "files_size_ratio": 0.75,
  "save_audio_file": true,
  "save_every_file": false,
  "delete_files_timeout_millisec": " 2 * 60 * 1000",
  "log_file": "./lib/app.log",
  "errors_log_file": "./lib/errors.log",
  "applications": "./Applications",
  "musescore_path": "./Applications/MuseScore 4/bin/MuseScore4.exe"
